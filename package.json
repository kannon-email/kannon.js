{
  "name": "kannon.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/kannon-email/kannon.js"
  },
  "version": "0.1.3",
  "description": "Kannon node client libreary",
  "main": "lib/kannon.js",
  "types": "lib/kannon.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
