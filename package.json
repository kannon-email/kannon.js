  "author": "ludusrusso",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "prettier": "^2.8.1",
    "ts-proto": "^1.137.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.1",
    "@connectrpc/connect": "^1.6.1",
    "@connectrpc/connect-node": "^1.6.1"
